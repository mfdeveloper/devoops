{
  "name": "devoops-bootstrap-theme",
  "description": "This is free open-source bootstrap 3 admin dashboard theme",
  "homepage": "http://devoops.me",
  "authors": [
    "Devoops <devoopsme@gmail.com>"
  ],
  "main": [
    "js/devoops.js",
    "css/style_v1.css",
    "css/style_v2.css"
  ],
  "dependencies": {
    "jquery": "~2.1.4",
    "jquery-ui": "~1.11.1",
    "bootstrap": ">= 3.0.0",
    "bootstrap-sass": "~3.3.5",
    "bootstrapvalidator": "~0.5.3",
    "fontawesome": ">=4.4.0",
    "droidSans": "https://google-fonts.azurewebsites.net/googleFonts/openSans?family=Righteous",
    "fancybox": "~2.1.5",
    "fullcalendar": "~2.4.0",
    "bower-xcharts": "git://github.com/vperron/bower-xcharts#0.3.0",
    "select2": "~4.0.0",
    "Justified-Gallery": ">=3.6.0",
    "chartist": "~0.9.4",
    "amcharts": "~3.15.2",
    "tinymce": ">= 4.2.5",
    "bower-jquery-sparkline": ">= 2.1.3",
    "d3": "~3.5.6",
    "datatables": ">=1.10.9",
    "datatables-tabletools":">=2.2.4",
    "fine-uploader": ">=5.3.1",
    "flot": "~0.8.3",
    "jquery-knob": "~1.2.11",
    "jqueryui-timepicker-addon": ">=1.5.5",
    "leaflet": "~0.7.5",
    "moment": "~2.10.6",
    "morris.js": ">=0.5.0",
    "openlayers": "*",
    "raphael": ">= 2.0",
    "springy": ">=2.7.1"
  },
  "keywords": [
    "devoops",
    "dashboard-theme",
    "admin-theme",
    "bootstrap-theme"
  ],
  "license": "GPL-3.0",
  "ignore": [
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "amcharts": {
      "main": [
        "dist/amcharts/amcharts.js",
        "dist/amcharts/funnel.js",
        "dist/amcharts/serial.js",
        "dist/amcharts/pie.js"
      ]
    },
    "droidSans": {
      "main": "righteous.css"
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js"
      ]
    },
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss"
      ]
    },
    "bootstrapvalidator": {
      "main": [
        "dist/css/bootstrapValidator.min.css",
        "dist/js/bootstrapValidator.min.js"
      ]
    },
    "bower-xcharts": {
      "main":[
        "xcharts.min.css",
        "xcharts.min.js"
      ]
    },
    "d3": {
      "main": [
        "d3.min.js"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.min.css",
        "media/js/jquery.dataTables.min.js"
      ]
    },
    "fancybox": {
      "main": [
        "source/jquery.fancybox.pack.js"
      ]
    },
    "fontawesome": {
      "main": [
        "css/font-awesome.min.css"
      ]
    },
    "fine-uploader": {
      "main": [
        "_build/jquery.fine-uploader.min.js"
      ]
    },
    "fullcalendar": {
      "main": [
        "dist/fullcalendar.min.css",
        "dist/fullcalendar.min.js"
      ]
    },
    "flot": {
      "main": [
        "jquery.flot.js",
        "jquery.flot.resize.js",
        "jquery.flot.time.js"
      ]
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "jquery-ui": {
      "main": [
        "themes/smoothness/jquery-ui.min.css",
        "jquery-ui.min.js"
      ]
    },
    "jquery-knob": {
      "main": [
        "dist/jquery.knob.min.js"
      ]
    },
    "jqueryui-timepicker-addon": {
      "main": [
        "dist/jquery-ui-timepicker-addon.min.css",
        "dist/jquery-ui-timepicker-addon.min.js"
      ]
    },
    "Justified-Gallery": {
      "main": [
        "dist/css/justifiedGallery.min.css",
        "dist/js/jquery.justifiedGallery.min.js"
      ]
    },
    "leaflet": {
      "main": [
        "dist/leaflet.css",
        "dist/leaflet.js"
      ]
    },
    "morris.js": {
      "main": [
        "morris.css",
        "morris.min.js"
      ]
    },
    "mocha": {
      "main": []
    },
    "openlayers": {
      "main": [
        "lib/OpenLayers.js"
      ]
    },
    "raphael": {
      "main": [
        "raphael-min.js"
      ]
    },
    "select2": {
      "main": [
        "dist/css/select2.min.css",
        "dist/js/select2.full.min.js"
      ]
    },
    "springy": {
      "main": [
        "springy.js",
        "springyui.js"
      ]
    }
  }
}
